{
  "name": "dptsi/laravel-sso",
  "description": "Laravel authentication package for SSO ITS",
  "type": "library",
  "authors": [
    {
      "name": "DPTSI ITS",
      "email": "developer@its.ac.id"
    }
  ],
  "require": {
    "php": "^8.0.2",
    "laravel/framework": "^9.21|^10.0",
    "myits/openid-connect-client": "^1.4",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Dptsi\\Sso\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dptsi\\Sso\\Providers\\SsoServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
