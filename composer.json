{
  "name": "dptsi/laravel-sso",
  "description": "Laravel authentication package for SSO ITS",
  "type": "library",
  "authors": [
    {
      "name": "DPTSI ITS",
      "email": "developer@its.ac.id"
    }
  ],
  "require": {
    "php": ">=7.4",
    "laravel/framework": ">=8.12",
    "ext-json": "*",
    "jumbojett/openid-connect-php": "^1.0.2"
  },
  "autoload": {
    "psr-4": {
      "Dptsi\\Sso\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dptsi\\Sso\\Providers\\SsoServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
